@model IEnumerable<Product>
<h2>My Products</h2>

<table>
    <tr>
        <th>Name</th>
        <th>Category</th>
        <th>Production Date</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Category</td>
            <td>@item.ProductionDate.ToShortDateString()</td>
        </tr>
    }
</table>